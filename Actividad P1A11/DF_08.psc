//Calcular la suma de los cincuenta primeros números enteros.
Algoritmo DF_08
	
	X <- 1;
	S <- 0;
	
	Mientras X<=50 Hacer;
		S <- S+X;
		X <- X+1;
	FinMientras;
	
	Escribir 'La suma de los primeros 50 números enteros es de: ', S;

FinAlgoritmo